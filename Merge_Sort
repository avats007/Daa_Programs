#include <iostream>

using namespace std;
void merge(int a[],int i,int m,int o,int j)
{
    int k=0;
    while(i<m && o<j)
    {
        if(a[i]<=a[j])
        {
            a[k] = a[i];
            i++;
        }
        else
        {
            a[k]=a[j];
            j++;
        }
        
        k++;
    }
    
    while(i<m)
    {
        a[k]=a[i];
        i++;
        k++;
    }
    
    while(o<j)
    {
        a[k]=a[j];
        j++;
        k++;
    }
}
void merge_sort(int a[],int i,int j)
{
    if(i==j)
    return ;
    
    else
    {
        int mid = (i+j)/2;
        
        merge_sort(a,i,mid);
        merge_sort(a,mid+1,j);
        
        merge(a,i,mid,mid+1,j);
    }
    
}
int main()
{
    int n;
    cout<<"Enter the size of the array\n";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
   int i=0,j=n-1;
   merge_sort(a,i,j);
    for(int i=0;i<n;i++)
    {
        cout<<a[i]<<" ";
    }
    return 0;
}
